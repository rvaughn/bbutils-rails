<h3>Groups</h3>

<div class="panel panel-default">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @groups.each do |group| %>
      <tr>
        <td><%= link_to group.name, group_path(group.slug) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
